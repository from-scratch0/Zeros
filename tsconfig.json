{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true, 
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      },
      "removeComments": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "allowJs": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    },
}